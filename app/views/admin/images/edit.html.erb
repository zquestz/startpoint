<h2><%= t(:edit_image) -%></h2>
<% form_for(@image, :url => admin_image_path(@image), :html => { :multipart => true }) do |f| %>
  <%- field_set_tag do -%>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <div class="bs">
      <%= f.submit t(:update) %>&nbsp;&nbsp;- <%= t(:or) -%> -&nbsp;&nbsp;<%= link_to(t(:cancel), safe_location(admin_images_path)) -%>
    </div>
  <%- end -%>
<% end %>